<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
	"http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
<!-- 郑成斌配置文件 -->
	<package name="zcb" namespace="/" extends="struts-default,json-default">
		<action name="assetSortAction_*" class="assetSortAction" method="{1}">
			<result name="list">/WEB-INF/page/assetSort_information/list.jsp</result>
			<result name="add">/WEB-INF/page/assetSort_information/addSort.jsp</result>
			<result name="success">/WEB-INF/page/assetSort_information/success.jsp</result>
			<result name="error">/WEB-INF/page/assetSort_information/error.jsp</result>
			<result name="update">/WEB-INF/page/assetSort_information/updateSort.jsp</result>
		</action>
		
		<action name="allocateAction_*" class="" method="{1}">
			<result name="list">/WEB-INF/page/allocate/list.jsp</result>
			<result name="add">/WEB-INF/page/allocate/add.jsp</result>
		</action>
	</package>
</struts>